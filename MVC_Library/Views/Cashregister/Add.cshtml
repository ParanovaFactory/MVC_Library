@model MVC_Library.Models.Entity.TblCashregister

@{
    ViewBag.Title = "Add";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<form class="form-group" method="post">
    <div>
        <label>Date</label>
        <input type="text" class="form-control" name="cashregisterMonth" />
    </div>
    <div>
        <label>Amount</label>
        <input type="text" class="form-control" name="cashregisterAmount" />
    </div>
    <div style="margin-top:10px">
        <button class="btn btn-success">SAVE</button>
    </div>
</form>